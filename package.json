{
  "name": "bloglist",
  "version": "1.0.0",
  "author": {
    "name": "Emmanuel O. Adeyeye"
  },
  "repository": {
    "url": "https://github.com/purrrplelipton/bloglist"
  },
  "private": "true",
  "scripts": {
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "server:dev": "cd server && yarn dev",
    "client:dev": "cd client && yarn dev",
    "server": "cd server && yarn start",
    "client": "cd client && yarn start",
    "build": "cd client && rimraf dist && yarn build && cd ../server && rimraf dist && cd ../client && mv dist ../server"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.1"
  },
  "license": "MIT"
}
